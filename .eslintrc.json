{
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "react/prop-types": "off",
      "no-unused-vars": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }